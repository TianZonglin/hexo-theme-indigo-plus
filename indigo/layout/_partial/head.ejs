<head>

<meta charset="utf-8">
<%
var keyWords = config.keywords;
if(page.tags){
	keyWords = [];
	_.isArray(page.tags) ? ( keyWords = page.tags )
		: page.tags.each(function(k){
			keyWords.push(k.name);
		});
	keyWords += ',';
}
%>
<meta name="author" content="田宗霖"> 

<meta name="keywords" content="<%= keyWords %>A,TZLoop,田宗霖的博客,田宗霖,whereareyou,大数据,前端,js,jquery,hexo,node,java,spark,scala,idea,vmware,hadoop,flume,kafka,oracle,个人博客,原创文章,图布局,大数据技术,论文分享,资源下载,图片分享,电影品评">

<meta itemprop="image" content="http://ozmhkse3b.bkt.clouddn.com/image/gif/friend.png" />  

<% if(!page.description){ %>
<meta name="description" content="欢迎访问TZLoop的博客，关注大数据、可视化、Java及前端技术，记录生活发现新鲜事，个人博客,原创文章,图布局,大数据技术,论文分享,资源下载,图片分享,电影品评" />
<% }%>
<%- open_graph({twitter_id: theme.twitter, google_plus: theme.google_plus, fb_admins: theme.fb_admins, fb_app_id: theme.fb_app_id}) %>

<% if(theme.google_site_verification){ %>
<meta name="google-site-verification" content="<%- theme.google_site_verification%>">
<% }%>
<%
	var title = page.title;

	if (is_archive()){
		title = theme.archives_title || 'Archives';

		if (is_month()){
		title += ': ' + page.year + '/' + page.month;
		} else if (is_year()){
		title += ': ' + page.year;
		}
	} else if (is_category()){
		title = (theme.categories_title || 'Categories') + ': ' + page.category;
	} else if (is_tag()){
		title = (theme.tags_title || 'Tags') + ': ' + page.tag;
	}
%>
<title><% if (title){ %><%= title %> | <% } %><%= config.title %><% if (config.subtitle){ %> | <%= config.subtitle %><% } %></title>

<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
<% if(theme.color){ %>
<meta name="theme-color" content="<%=theme.color%>">
<% } %>

<% if(theme.canonical){ %>
<link rel="canonical" href="<%- (theme.canonical + url_for(page.path)).replace(/index\.html$/, '') %>">
<% }%>

<% if ((config.feed) && (config.feed.path.length)) { %>
<link rel="alternate" type="application/atom+xml" title="<%= config.title %>" href="<%- url_for(config.feed.path) %>">
<% } %>
<%- favicon_tag(theme.favicon) %>
<link rel="stylesheet" href="/css/style.css">
<script>window.lazyScripts=[]</script>
<%- partial('plugins/google-analytics') %>
<%- partial('plugins/tajs') %>
<%- partial('plugins/baidu') %>

<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<script>
  (adsbygoogle = window.adsbygoogle || []).push({
	google_ad_client: "ca-pub-9432057653708398",
	enable_page_level_ads: true
  });
  console.log("Adsbygoogle done!");
</script>


